{
  "name": "helpdesk",
  "version": "2.0.0",
  "description": "A HelpDesk System for Bioverse",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": " node server/index.js --ignore dist/ --ignore src/ ",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "start-server:windows": "cross-env JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "db:build": "node ./script/seed.js"
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/react-redux": "^7.1.28",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.11",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.3.2",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "framer-motion": "^10.16.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^5.3.3",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "sequelize": "^6.21.4",
    "typescript": "^5.2.2"
  },
